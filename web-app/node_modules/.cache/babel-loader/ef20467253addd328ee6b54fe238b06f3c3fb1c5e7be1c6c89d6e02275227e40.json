.$store.getters.option3\r\n    this.keyWord = this.$route.query?.keywords\r\n  },\r\n  methods: {\r\n    ...mapActions([\"updateOption1\", \"updateOption2\", \"updateOption3\"]),\r\n    toInformation(type) {\r\n      this.$router.push({\r\n        path:'/info',\r\n        query:{\r\n          type:type\r\n        }\r\n      })\r\n    },\r\n    onSelect1(option) {\r\n      this.optionSelect1 = !this.optionSelect1\r\n      this.option1 = option\r\n      this.updateOption1(this.option1)\r\n    },\r\n    onSelect2(option) {\r\n      this.optionSelect2 = !this.optionSelect2\r\n      this.option2 = option\r\n      this.updateOption2(this.option2)\r\n    },\r\n    onSelect3(option) {\r\n      this.optionSelect3 = !this.optionSelect3\r\n      this.option3 = option\r\n      this.updateOption3(this.option3)\r\n    },\r\n    toNext() {\r\n      if(!this.option1 || !this.option2 || !this.option3) {\r\n        this.isNext = true\r\n        this.$message.warning(`You haven't finished selecting, please continue selecting`);\r\n        return\r\n      }\r\n\r\n      this.isNext = false\r\n      \r\n      this.$router.push({\r\n        path: '/tuitionInquiry',\r\n        query: {\r\n          keyWord: this.keyWord\r\n        }\r\n      })\r\n    },\r\n    returnBtn() {\r\n      this.$router.push({\r\n        path: '/'\r\n      })\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.preparation-page {\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: hidden;\r\n  .img-box {\r\n    position: absolute;\r\n    top:10px;\r\n    left: 0px;\r\n  }\r\n  .header {\r\n    background: #FF8D1A;\r\n    height: 250px;\r\n    display: flex;\r\n    justify-content: center;\r\n    .head-box {\r\n      width: 800px;\r\n      margin-top: 50px;\r\n      .title {\r\n        font-size: 24px;\r\n        font-weight: 900;\r\n        color: #000;\r\n        display: flex;\r\n        justify-content: center;\r\n      }\r\n      .sub-title {\r\n        display: flex;\r\n        justify